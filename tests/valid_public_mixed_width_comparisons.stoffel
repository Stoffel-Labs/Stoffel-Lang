proc main() =
  let i8v: int8 = 10i8
  let i16v: int16 = 10i16
  let i32v: int32 = 100i32
  let i64v: int64 = 100i64
  let u8v: uint8 = 200u8
  let u16v: uint16 = 200u16
  let u32v: uint32 = 200u32
  let u64v: uint64 = 200u64
  let s: secret int64 = 0

  if i8v <= i16v:
    discard s

  if i32v < i64v:
    discard s

  if u8v == 200u8:
    discard s

  if u16v != u32v:
    discard s
