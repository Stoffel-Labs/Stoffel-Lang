# Recursive Fibonacci sample that computes fib(10) = 55

def fib_tr(n: int64, a: int64, b: int64) -> int64:
  if n == 0:
    return a
  else:
    # Tail call: last operation is the recursive call itself
    return fib_tr(n - 1, b, a + b)

def fib(n: int64) -> int64:
  return fib_tr(n, 0, 1)

main main() -> nil:
  var result = fib(10)
  discard result
