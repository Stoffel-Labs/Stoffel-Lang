# Fibonacci sample that computes fib(10) = 55
# Uses an iterative approach with a running pair (a, b).

main main() -> nil:
  var n = 10
  var result = 0

  if n == 0:
    result = 0
  else:
    if n == 1:
      result = 1
    else:
      var a = 0
      var b = 1

      for i in 2..n:
        var next = a + b
        a = b
        b = next

      result = b

  discard result
