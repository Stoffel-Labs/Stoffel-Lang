# Fibonacci sample that returns 55 for input n=10
# Uses an iterative approach with a running pair (a, b).

main main() -> int64:
  var n = 10

  if n == 0:
    return 0
  else:
    if n == 1:
      return 1
    else:
      var a = 0
      var b = 1

      for i in 2..n:
        var next = a + b
        a = b
        b = next

      return b
